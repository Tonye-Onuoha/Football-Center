{

    "builds": [{

        "src": "Fan/wsgi.py",

        "use": "@vercel/python",

        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }

    }],

    "routes": [

        {

            "src": "/(.*)",

            "dest": "Fan/wsgi.py"

        }

    ],
	
	"functions": {

    	"api/test.js": {

      	"maxDuration": 30 // This function can run for a maximum of 30 seconds

    },

    "api/*.js": {

      	"maxDuration": 15 // This function can run for a maximum of 15 seconds

    }

  }

}